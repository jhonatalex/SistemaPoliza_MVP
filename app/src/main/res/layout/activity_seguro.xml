<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D8D9DA"
    tools:context=".ActivityTransito">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="331dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/round_bg"
        android:gravity="center_horizontal|center_vertical"
        android:text="Numero de Accidente"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/totalP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="272dp"
        android:layout_marginTop="484dp"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="150dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/round_bg"
        android:gravity="center_horizontal|center_vertical"
        android:text="Propietario"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="223dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/round_bg"
        android:gravity="center_horizontal|center_vertical"
        android:text="Valor del Auto"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="492dp"
        android:text="Costo Total de Poliza:"
        android:textColor="#1E3D9A"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="372dp"
        android:layout_height="62dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="536dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/s_calcular"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="128dp"
            android:layout_height="wrap_content"
            android:text="Calcular"
            android:textColor="#000000" />

        <Button
            android:id="@+id/s_nuevo"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:text="Nuevo"
            android:textColor="#000000" />

        <Button
            android:id="@+id/s_salir"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:text="Salir"
            android:textColor="#000000" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="325dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/degrado"
        android:gravity="center_horizontal|center_vertical"
        android:text="SEGURO DE AUTOS"
        android:textColor="#050505"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/text_propietario"
        android:layout_width="176dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="136dp"
        android:background="@drawable/round_border"
        android:ems="10"
        android:gravity="center_horizontal|center_vertical"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txt_valorauto"
        android:layout_width="250dp"
        android:layout_height="32dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="214dp"
        android:background="@drawable/round_border"
        android:ems="10"
        android:gravity="center_horizontal|center_vertical"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/text_accidente"
        android:layout_width="357dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="292dp"
        android:background="@drawable/round_border"
        android:ems="10"
        android:gravity="center_horizontal|center_vertical"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioGroup
        android:layout_width="146dp"
        android:layout_height="107dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="364dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/modeloA"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MODELO A" />

        <RadioButton
            android:id="@+id/modeloB"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MODELO B" />

        <RadioButton
            android:id="@+id/modeloC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MODELO C" />
    </RadioGroup>

    <RadioGroup
        android:layout_width="209dp"
        android:layout_height="111dp"
        android:layout_marginStart="185dp"
        android:layout_marginTop="364dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/menor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mayor o = 18 y Menor a 24" />

        <RadioButton
            android:id="@+id/medio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mayor 0 = 24 y Menor a 53" />

        <RadioButton
            android:id="@+id/mayor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mayor o = 53 " />
    </RadioGroup>


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="335dp"
        android:text="MODELO DE AUTO"
        android:textColor="#05149A"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="196dp"
        android:layout_marginTop="335dp"
        android:text="EDAD DEL PROPIETARIO"
        android:textColor="#05149A"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="144dp"
        android:layout_height="189dp"
        android:layout_marginStart="243dp"
        android:layout_marginTop="57dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/seg" />

</androidx.constraintlayout.widget.ConstraintLayout>